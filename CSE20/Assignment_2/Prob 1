num1 = int(input("Enter first int: "))
num2 = int(input("Enter second int: "))

i = 1
while i <= 20:
    if i % num1 == 0 and i % num2 == 0:
        print('FIZZBUZZ', end = ' ')
    elif i % num1 == 0:
        print('FIZZ', end = ' ')
    elif i % num2 == 0:
        print('BUZZ', end = ' ')
    else:
        print(i, end = ' ')
    i += 1
